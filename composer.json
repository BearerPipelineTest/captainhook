{
  "name": "sebastianfeldmann/hookmeup",
  "description": "PHP git hook manager",
  "type": "library",
  "keywords": [
    "git",
    "hooks",
    "pre-commit",
    "pre-push"
  ],
  "homepage": "https://github.com/sebastianfeldmann/hookmeup",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Feldmann",
      "email": "sf@sebastian-feldmann.info",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/sebastianfeldmann/hookmeup/issues"
  },
  "autoload": {
    "psr-4": {
      "HookMeUp\\": "src/"
    }
  },
  "require": {
    "ext-json": "*",
    "ext-spl": "*",
    "symfony/console": "3.*",
    "symfony/process": "3.*"
  },
  "bin": [
    "hookmeup"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
